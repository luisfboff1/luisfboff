    "lucide-react": "^0.544.0",
    "mysql2": "^3.11.5",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.2.0",
    "sqlite3": "^5.1.7",
